{
	"name" : "coka/file-bundle",
	"type" : "symfony-bundle",
	"description" : "Symfony Oka file bundle",
	"homepage" : "https://github.com/CedrickOka/file-bundle",
	"license" : "MIT",
	"keywords" : [
		"file",
		"image",
		"audio",
		"video",
		"upload",
		"thumbnail",
		"cropping",
		"behaviors"
	],
	"authors" : [{
			"name" : "Cedrick Oka",
			"email" : "okacedrick@gmail.com"
		}
	],
	"require" : {
		"php" : ">=5.6",
		"coka/pagination-bundle" : "^3.0",
		"imagine/imagine" : "^0.6.3",
		"symfony/framework-bundle" : "^3.4 || ^4.0",
		"symfony/event-dispatcher" : "^3.4 || ^4.0",
		"symfony/dependency-injection" : "^3.4 || ^4.0",
		"symfony/console" : "^3.4 || ^4.0",
		"symfony/config" : "^3.4 || ^4.0",
		"symfony/form" : "^3.4 || ^4.0",
		"symfony/translation" : "^3.4 || ^4.0",
		"symfony/validator" : "^3.4 || ^4.0",
		"ksubileau/color-thief-php" : "^1.4"
	},
	"require-dev" : {
		"symfony/phpunit-bridge" : "^4.2",
		"symfony/orm-pack" : "^1.0",
		"alcaeus/mongo-php-adapter" : "^1.1",
		"doctrine/mongodb-odm-bundle" : "^3.5"
	},
	"suggest" : {
		"ext-gd" : ">=2.2.0",
		"ext-gmagick" : ">=2.0.4",
		"ext-imagick" : ">=3.1.2",
		"alcaeus/mongo-php-adapter" : "^1.1",
		"doctrine/doctrine-bundle" : "~1.4",
		"doctrine/mongodb-odm-bundle" : "^3.4",
		"doctrine/orm" : "^2.4.8",
		"twig/twig" : "^2.5"
	},
	"autoload" : {
		"psr-4" : {
			"Oka\\FileBundle\\" : "src/"
		},
		"exclude-from-classmap" : [
			"/tests/"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"Oka\\FileBundle\\Tests\\" : "tests/"
		}
	},
	"prefer-stable" : true,
	"extra" : {
		"branch-alias" : {
			"dev-master" : "3.0.x-dev"
		},
		"symfony" : {
			"allow-contrib" : "true"
		}
	},
	"support" : {
		"docs" : "https://github.com/CedrickOka/file-bundle/blob/master/src/Resources/doc/index.md"
	},
	"config" : {
		"platform" : {
			"ext-mongo" : "1.6.16"
		}
	}
}
