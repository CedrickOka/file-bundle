{
	"name" : "coka/file-bundle",
	"description" : "Symfony Oka file bundle",
	"type" : "symfony-bundle",
	"license" : "MIT",
	"keywords" : [
		"file",
		"image",
		"audio",
		"video",
		"upload",
		"manipulation",
		"thumbnail",
		"cropping",
		"behaviors"
	],
	"authors" : [{
			"name" : "Cedrick Oka",
			"email" : "okacedrick@gmail.com"
		}
	],
	"homepage" : "https://github.com/CedrickOka/oka-file",
	"require" : {
		"php" : "^5.5 || ^7.0",
		"coka/pagination-bundle" : "^2.0",
		"doctrine/orm" : "^2.4.8",
		"doctrine/doctrine-bundle" : "~1.4",
		"imagine/imagine" : "^0.6.3",
		"symfony/symfony" : "^3.4"
	},
	"require-dev" : {
		"symfony/phpunit-bridge" : "^3.0"
	},
	"suggest" : {
		"ext-gd" : ">=2.2.0",
		"ext-imagick" : ">=3.1.2"
	},
	"autoload" : {
		"psr-4" : {
			"Oka\\FileBundle\\" : "src"
		},
		"exclude-from-classmap" : [
			"/Tests/"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"Oka\\FileBundle\\Tests\\" : "tests"
		}
	},
	"prefer-stable" : true,
	"minimum-stability" : "dev",
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.x.x-dev"
		}
	},
	"support" : {
		"docs" : "https://github.com/CedrickOka/oka-file/blob/master/Resources/doc/index.md"
	}
}

